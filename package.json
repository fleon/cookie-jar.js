    "test": "npm run build-test; testem ci",
    "lint": "jshint src test",
    "testem": "testem"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jasmine-core": "^2.4.1",
    "jshint": "^2.9.2",
    "testem": "^1.6.0",
    "uglify-js": "^2.6.2"
  }
}
