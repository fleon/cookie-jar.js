{
  "name": "cookie-jar",
  "version": "0.0.1",
  "description": "A library to bucket multiple cookies in a single cookie with fast storage and retrieval.",
  "main": "index.js",
  "scripts": {
    "build": "browserify src -o dist/ampere.js -s ampere",
